namespace protocol;

table Handshake {
  clientVersion:ushort;

  hwid:string;
  user:string;
  pass:string; // hashed password

  dbVersion:ushort;
  dbMD5:string; // TODO: optimize
  dbName:string;
}

root_type Handshake;